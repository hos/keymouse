# Example configuration

# Global options
device=/dev/input/event1
udevice=/dev/input/uinput
debug=1
dump=0
max accel=200
max modes=6

# 0 no notify 1 blocknum notify 2 exec notify
notify=0
notify exec=

# Mapping (see /usr/include/linux/input.h for keycode or use dump option)
# m=apple key key M=fn + ctrl (iBook G3 keyboard)
# modes=<c|s|a|m|M|f>
# bkeys=<fc|fs|fa|fm>

modes=29,42,56,125,464
bkeys=97,54,100,126

# Actions
# <action>=mod1:mod2

up=42:0:
down=29:1:
right=56:0:
left=29:2:
rup=42:3:
rdown=125:1:
lup=42:2:
ldown=56:1:
click=97:4:
rclick=126:4:
3click=100:4:
lock=54:4:

# EOF
